# Copy this file to .env and adjust values for your environment.
# Application env vars prefixed with VITE_ will be exposed to the frontend at build time.

# Base URL for the backend API used by the image annotator.
VITE_API_BASE_URL=https://api.example.com

# Optional auth token forwarded with API calls. Leave blank to disable auth headers.
VITE_API_AUTH_TOKEN=

# Port exposed by the container runtime server.
PORT=8080
